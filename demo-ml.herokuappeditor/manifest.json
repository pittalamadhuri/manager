{
  "manifest_version": 2,
  "name": "LetzNav Editor",
  "version": "2.0.1746",
  "description": "",
  "icons": {
    "16": "assets/icon/favicon-16x16.png",
    "32": "assets/icon/favicon-32x32.png",
    "96": "assets/icon/favicon-96x96.png"
  },
  "permissions": [
    "<all_urls>",
    "browsingData",
    "cookies",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
  "web_accessible_resources": [
    "*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "polyfills.bundle.js",
        "vendor.bundle.js",
        "main.bundle.js",
        "libraries.bundle.js"
      ],
      "css": [
        "styles.bundle.css"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "background": {
    "scripts": [
      "polyfills.bundle.js",
      "vendor.bundle.js",
      "background.bundle.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "LetzNav Editor"
  },
  "update_url": "https://demo-ml.herokuapp.com/api/public/admin/extensions/editor/updates.xml"
}